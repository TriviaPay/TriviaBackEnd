{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python",
      "config": {
        "skipBuild": true
      }
    }
  ],
  "functions": {
    "main.py": {
      "excludeFiles": "{.trace_coverage_*/**,tests/**,test_*.py,*.log,server_log.txt,README*.md,*.md,.pytest_cache/**,__pycache__/**,.pycache/**,.vercel/**,scripts/**,api/**}"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ],
  "env": {
    "PYTHONPATH": ".",
    "TESTING": "false",
    "SKIP_TESTS": "true"
  }
}
