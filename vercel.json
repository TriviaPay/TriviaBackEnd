{
    "version": 2,
    "builds": [
        {
            "src": "api/index.py",
            "use": "@vercel/python@3.1.7",
            "config": {
                "includeFiles": [
                    "api/build.sh"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/index.py"
        }
    ]
} 