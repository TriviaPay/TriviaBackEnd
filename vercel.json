{
    "version": 2,
    "builds": [
        {
            "src": "main.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb"
            }
        }
    ],
    "routes": [
        {
            "src": "/docs",
            "dest": "main.py"
        },
        {
            "src": "/openapi.json",
            "dest": "main.py"
        },
        {
            "src": "/redoc",
            "dest": "main.py"
        },
        {
            "src": "/(.*)",
            "dest": "main.py"
        }
    ]
} 